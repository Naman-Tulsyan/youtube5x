{
  "manifest_version": 3,
  "name": "YouTube 5x",
  "description": "Speed up YouTube videos upto 5x. Helpful in your exam days ;)",
  "version": "1.0",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
     "default_icon": "logo.png"
    },
    "icons": {
        "128" : "logo.png"
    }
  
}
